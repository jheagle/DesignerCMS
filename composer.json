{
  "name": "joshuaheagle/designer-cms",
  "description": "PHP + MySQL Datatype integration. Functional programming building blocks.",
  "type": "library",
  "authors": [
    {
      "name": "Joshua Heagle",
      "email": "joshuaheagle@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Core\\Controllers\\": "core/controllers/",
      "Core\\Database\\": "core/database/",
      "Core\\DataTypes\\": [
        "core/datatypes/",
        "core/datatypes/interfaces"
      ],
      "Core\\DataTypes\\Numbers\\": [
        "core/datatypes/numbers/",
        "core/datatypes/numbers/integers",
        "core/datatypes/numbers/doubles"
      ],
      "Core\\DataTypes\\Strings\\": "core/datatypes/strings/",
      "Core\\Entity\\": "core/entity/",
      "Core\\Models\\": "core/models/",
      "Core\\Utilities\\": "core/utilities/",
      "Core\\Utilities\\Functional\\": "core/utilities/functional/",
      "Core\\Utilities\\Traits\\": "core/utilities/traits/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Core\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^7.4",
    "guzzlehttp/guzzle": "~6.0",
    "league/oauth2-client": "^2.4",
    "ext-json": "^7.4"
  },
  "require-dev": {
    "codedungeon/phpunit-result-printer": "^0.23",
    "fzaninotto/faker": "^1.8",
    "mockery/mockery": "^1.2",
    "phpunit/phpunit": "^7",
    "twig/twig": "^2.0",
    "symfony/var-dumper": "^4.3"
  }
}
